
body { font-family: Geogia,Arial, Helvetica, sans-serif; font-size: 16px; ; line-height: ; background-color: #F7FFED;}
*{ margin: 0; padding: 0; }
.clear { clear: both; }
a:link,a:hover,a:active,a:visited { color: #0058FF; }
.fl { float: left; }
.up { text-transform: uppercase; }
.center { text-align: center; }
.clearfix:after { content:""; display:table; clear:both;}
.clearfix { zoom:1; }

.col-10 { width: 10%; }

.col-20 { width: 20%; }
.col-25 { width: 25%; }
.col-30 { width: 30%; }
.col-40 { width: 40%; }

.col-70 { width: 70%; }

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none !important;
}
input[type="number"] {
   -moz-appearance: textfield;
}

input { width: 100%; padding: 10px; font-size: 18px; box-sizing:border-box; }
select { width: 100%; padding: 10px; font-size: 18px; left: 0; }
h1,h2 { font-weight: 400;}

.wrap { width: 500px; padding: 20px; margin: 50px auto 0; background-color: #fff; }
.wrap section { margin-bottom: 50px; display: none; }
.wrap section.active { display: block; }

.wrap > div { margin: 30px 0; }
.wrap .title span { float: left; width: 30px; height: 30px; margin-right: 20px; line-height: 30px; text-align: center; font-size: 24px; border-radius: 50%; color: #fff; background-color: #123;}
.wrap .title h1 { line-height: 30px; font-weight: 400;}
.wrap .hide { display: none; }

.wrap .tips { background-color: #CACACA; padding: 10px 0; margin: 30px 0; }
.wrap .tips img { float: left; width: 24px; margin:0 8px 0 12px; }
.wrap .tips p { line-height: 24px; }

.wrap .form .entry { margin: 30px 0; position: relative; }
.wrap .form .entry.fl { margin: 0 0 30px; }

.wrap .form .preview p { padding: 10px; background-color: #cfcfcf; font-size: 20px; line-height: 20px; text-align: center; }

.wrap .form label { display: block; margin-bottom: 8px; width: 100%; }
.wrap .form label.required:after { content: '*'; color: #f00; }
.wrap .form .btn { float: left; width: 50%; box-sizing:border-box; }
.wrap .form .btn a { display: block; height: 50px; line-height: 50px; text-decoration: none; color: #fff; background-color: #387933; text-align: center; }
.wrap .form .btn.submit a { background-color: #333979; }
.wrap .form .checkbox input { float: left; width: auto; width: 20px; height: 20px; margin-right: 10px; }

.wrap .text h2 { margin: 30px 0 50px; }
.wrap .text p { margin: 30px 0; line-height: 1.6em;}

div.dz-error-mark > p { padding: 10px 0; text-align: center; }

span.customSelect { font-size: 18px; background-color: #fff; color: #000; padding: 10px; border:1px solid #ddd; height: 45px; box-sizing: border-box; }
span.customSelect.changed { background-color: #fff; }
.customSelectInner { background:url(../images/customSelect-arrow.gif) no-repeat center right; }









